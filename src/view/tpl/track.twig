<!-- Google Tag Manager -->
{% if session.loggedIn %}
{% set gtm_userId = session.userId %}
{% else %}
{% set gtm_userId = '(not set)' %}
{% endif %}
{% if client is defined and client is not empty %}
{% set gtm_clientId = client.id %}
{% set gtm_clientName = client.name %}
{% else %}
{% set gtm_clientId = '(not set)' %}
{% set gtm_clientName = '(not set)' %}
{% endif %}
<script type="text/javascript">
  window.dataLayer = [{
    'client-id': '{{ gtm_clientName }}',
    'client-name': '{{ gtm_clientId }}',
    'user-id': '{{ gtm_userId }}'
  }];
</script>
